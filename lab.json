{
  "name": "jxnblk.github.io",
  "library": "styled-components",
  "components": [
    {
      "name": "Avatar",
      "filename": "Avatar.js",
      "external": true,
      "examples": [
        "<Avatar size={128} />"
      ],
      "system": []
    },
    {
      "name": "Box",
      "type": "div",
      "props": {},
      "style": {},
      "examples": [
        "<Box>Hello</Box>"
      ],
      "system": []
    },
    {
      "name": "Text",
      "type": "div",
      "props": {
        "fontWeight": "600"
      },
      "style": {},
      "examples": [
        "<Text>Hello</Text>"
      ],
      "system": [
        "fontWeight"
      ]
    },
    {
      "name": "Caps",
      "type": "Text",
      "props": {
        "fontSize": 10,
        "fontWeight": "700"
      },
      "style": {
        "textTransform": "uppercase",
        "letterSpacing": "0.15625em"
      },
      "examples": [
        "<Caps>\n  Hello\n</Caps>"
      ],
      "system": [
        "fontWeight"
      ]
    },
    {
      "name": "Link",
      "type": "a",
      "props": {
        "color": "inherit",
        "fontWeight": "600",
        "hover": {
          "webkitTextDecoration": "underline",
          "textDecoration": "underline",
          "color": "blue"
        }
      },
      "style": {
        "webkitTextDecoration": "none",
        "textDecoration": "none"
      },
      "examples": [
        "<Link href='#!'>Hello</Link>"
      ],
      "system": [
        "fontWeight",
        "hover"
      ]
    },
    {
      "name": "Title",
      "type": "h1",
      "props": {
        "fontSize": 4,
        "m": 0
      },
      "style": {
        "fontWeight": "bold",
        "lineHeight": "1.125"
      },
      "examples": [
        "<Title>Hello</Title>"
      ],
      "system": []
    },
    {
      "name": "Heading",
      "type": "h2",
      "props": {
        "fontSize": 4,
        "m": 0
      },
      "style": {
        "fontWeight": "bold",
        "lineHeight": 1.25
      },
      "examples": [
        "<Heading>Hello</Heading>"
      ]
    },
    {
      "name": "Container",
      "type": "Box",
      "props": {
        "mx": "auto",
        "px": 3
      },
      "style": {
        "maxWidth": "1024px"
      },
      "examples": [
        "<Container />"
      ],
      "system": []
    },
    {
      "name": "Image",
      "type": "img",
      "props": {
        "borderRadius": 8
      },
      "style": {
        "display": "block",
        "maxWidth": "100%",
        "height": "auto"
      },
      "examples": [
        "<Image\nsrc='http://jxnblk.com/rebass/card.png'\n/>"
      ],
      "system": [
        "borderRadius"
      ]
    },
    {
      "name": "BlockLink",
      "type": "a",
      "props": {
        "color": "inherit",
        "hover": {
          "color": "blue"
        },
        "focus": {
          "color": "blue"
        }
      },
      "style": {
        "display": "block",
        "webkitTextDecoration": "none",
        "textDecoration": "none"
      },
      "examples": [
        "<BlockLink>Hello</BlockLink>"
      ],
      "system": [
        "hover",
        "focus"
      ]
    },
    {
      "name": "BackgroundImage",
      "type": "div",
      "props": {
        "ratio": 0.5,
        "size": "cover",
        "borderRadius": 8
      },
      "style": {
        "width": "100%",
        "height": "0",
        "paddingBottom": "${(props.ratio * 100) + '%'}",
        "backgroundImage": "url(${props.src})",
        "backgroundPosition": "center",
        "backgroundSize": "${props.size}",
        "backgroundRepeat": "no-repeat"
      },
      "examples": [
        "<BackgroundImage\n  src='http://jxnblk.com/rebass/card.png'\n/>",
        "<BackgroundImage\n  size='contain'\n  src='http://jxnblk.com/rebass/card.png'\n/>"
      ],
      "system": [
        "borderRadius"
      ]
    },
    {
      "name": "ImageCard",
      "imports": [
        "Box",
        "Image",
        "Heading",
        "Text",
        "BlockLink",
        "BackgroundImage"
      ],
      "jsx": "<BlockLink href={props.href}>\n  <BackgroundImage\n    size={props.size}\n  ratio={3/4} src={props.image} mb={1} />\n  <Heading fontSize={2}>{props.heading}</Heading>\n  {props.text && <Text fontSize={0}>{props.text}</Text>}\n</BlockLink>",
      "examples": [
        "<ImageCard\n  image='http://jxnblk.com/rebass/card.png'\n  heading='Rebass'\n  text='Functional React UI component library, built with styled-components'\n/>"
      ],
      "system": []
    },
    {
      "name": "Pre",
      "type": "pre",
      "props": {},
      "style": {
        "fontFamily": "Menlo, monospace"
      },
      "examples": [
        "<Pre>Hello</Pre>"
      ],
      "system": []
    }
  ]
}